<?xml version = "1.0" encoding = "ISO-8859-1"?>

<!DOCTYPE controladores SYSTEM "dtds/controladores.dtd">


<controladores>

    <controlador nombre="Vehiculo">
        <hereda-de controlador="Typical"/>
        <accion nombre="Vehiculos por Año" icono="printer" modo = "list"
                clase="ni.edu.uam.SistemaContratoGPS.actions.PrintVehiclesByYear"/>
    </controlador>

    <controlador nombre="Contrato">
        <hereda-de controlador="Typical"/>
        <accion nombre="Contratos Por Vencer / Vencidos" icono="printer" modo = "list"
                clase="ni.edu.uam.SistemaContratoGPS.actions.PrintReporteContratosVencimiento"/>
        <accion nombre="Contratos Por Plan" icono="printer" modo = "list"
                clase="ni.edu.uam.SistemaContratoGPS.actions.PrintContratosPorPlan"/>
        <accion nombre="Top 10 Clientes" icono="account" modo = "list"
                clase="ni.edu.uam.SistemaContratoGPS.actions.PrintTop10ClientesContratos"/>
    </controlador>

</controladores>


